{
    "name": "sje-guedes/symfony-bm-api",
    "description": "Simple REST API project",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.0",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-http": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^2.2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0.2",
        "doctrine/orm": "2.7.*",
        "gesdinet/jwt-refresh-token-bundle": "^0.10.1",
        "jms/serializer-bundle": "^3.8",
        "lexik/jwt-authentication-bundle": "^2.10",
        "nelmio/api-doc-bundle": "^4.1",
        "ramsey/uuid": "^4.1",
        "ramsey/uuid-doctrine": "^1.6",
        "sensio/framework-extra-bundle": "^5.6",
        "symfony/asset": "5.0.*",
        "symfony/cache": "5.0.*",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/expression-language": "5.0.*",
        "symfony/flex": "^1.11.0",
        "symfony/framework-bundle": "5.0.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/security-bundle": "5.0.*",
        "symfony/stopwatch": "5.0.*",
        "symfony/twig-bundle": "5.0.*",
        "symfony/validator": "5.0.*",
        "symfony/yaml": "5.0.*",
        "willdurand/hateoas-bundle": "^2.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fakerphp/faker": "^1.13",
        "symfony/browser-kit": "5.0.*",
        "symfony/debug-bundle": "5.0.*",
        "symfony/maker-bundle": "^1.28",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/process": "5.0.*",
        "symfony/web-profiler-bundle": "5.0.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.4.0"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
